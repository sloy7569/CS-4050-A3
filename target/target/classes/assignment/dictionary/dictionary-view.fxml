<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="539.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="assignment.dictionary.DictionaryController">
   <children>
      <VBox prefHeight="398.0" prefWidth="712.0">
         <children>
            <HBox>
               <VBox.margin>
                  <Insets left="20.0" top="20.0" />
               </VBox.margin>
               <children>
                  <Button fx:id="checkSpellingBtn" alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#checkSpellingBtnClicked" text="Check Spelling" />
                  <Label fx:id="msg" alignment="CENTER" prefHeight="28.0" prefWidth="548.0" textAlignment="CENTER">
                     <font>
                        <Font name="System Bold" size="15.0" />
                     </font></Label>
               </children>
            </HBox>
            <VBox fx:id="view" prefHeight="310.0" prefWidth="498.0">
               <VBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="40.0" />
               </VBox.margin>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
